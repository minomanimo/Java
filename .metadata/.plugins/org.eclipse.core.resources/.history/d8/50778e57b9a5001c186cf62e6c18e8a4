
public class Ex02 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String yes = "예";
		for (int i = 5; i>0; i--)
		{
			System.out.println(yes);
		}
		for (int i = 0; i < 2 ; i++)
		{
			System.out.println("예"+i);
			for(int j = 0; j < 3; j++)
			{
				System.out.println("아니오"+j);
			}
		}
	}

}
